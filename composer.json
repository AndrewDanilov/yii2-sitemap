{
    "name": "andrewdanilov/yii2-sitemap",
    "description": "Component generates sitemap.xml for your models, controller actions or random pages.",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","sitemap","generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "AndrewDanilov",
            "email": "andrewdanilov@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "yiisoft/yii2": "~2.0.0",
        "ext-dom": "*"
    },
    "autoload": {
        "psr-4": {
            "andrewdanilov\\sitemap\\": "src/"
        }
    },
    "extra": {
        "bootstrap": "andrewdanilov\\sitemap\\Bootstrap"
    }
}
